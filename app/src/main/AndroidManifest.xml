<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.cairoguide">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Pharos"
            android:label="@string/Pharos"/>

        <activity android:name=".Religious"
            android:label="@string/Religious"/>

        <activity android:name=".Restaurants"
            android:label="@string/Restaurants"/>

        <activity android:name=".Shopping"
            android:label="@string/Shopping"/>
    </application>

</manifest>